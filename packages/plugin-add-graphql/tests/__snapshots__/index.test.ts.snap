exports[`simple objects example schema > generates expected schema 1`] = `
"type AddedUser implements Node {
  id: ID!
  name: String
  posts: [Post!]
  profile: Profile
}

type ExtraType {
  extra: String
}

type ExtraTypeFromSchema {
  extra: String
}

interface Node {
  id: ID!
}

type Post implements Node {
  content: String
  id: ID!
  postTitle: String!
}

type Profile implements Node {
  bio: String
  id: ID!
}

type Query {
  extra: ExtraType!
  extraFromSchema: ExtraTypeFromSchema!
  posts: [Post!]!
  user(id: ID!): AddedUser!
}"
`;
